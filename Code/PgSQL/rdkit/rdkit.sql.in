    RETURNS internal              -- state_data_type
    PARALLEL SAFE
    AS 'MODULE_PATHNAME'
    LANGUAGE C IMMUTABLE;
CREATE AGGREGATE fmcs (
    BASETYPE  = mol,       -- item type
